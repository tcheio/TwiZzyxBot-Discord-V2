function testTitre(msg) {
    statu = true;
        comp = 0
        i = 0;
        titre ="";
        msgBis = msg.split("");
        max = msgBis.length;
        //Exclure mon pseudo et les 2 symboles
        while (statu){
            if (msgBis[i] == "-"){
                comp +=1;
            }
    
            if (comp == 1){
                statu = false;
            }
            i++;
        }

        for (i; i < max; i++){
            if (msgBis[i] == "|"){
                return titre.trim();
            }
            titre += msgBis[i];
        }
}

function maxIntent(tab){
    max = 0;
    for (i=0; i<tab.length;i++){
        if (tab[i] > max){
            max = tab[i];
        }
    }
    return max;
}

